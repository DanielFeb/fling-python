{
    "version": 1,
    "formatters": {"default": {
        "format": "[%(asctime)s] %(levelname)s in %(module)s: %(message)s",
    }},
    "handlers": {"wsgi": {
        "class": "logging.StreamHandler",
        "stream": "ext://flask.logging.wsgi_errors_stream",
        "formatter": "default"
    }, "file": {
        "class": "logging.handlers.TimedRotatingFileHandler",
        "filename": logfile,
        "level": "DEBUG",
        "when": "M",
        "interval": 1,
        "backupCount": 0,
        "formatter": "default"
    }},
    "root": {
        "level": "INFO",
        "handlers": ["wsgi", "file"]
    }
}